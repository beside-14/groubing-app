{
  "name": "groubing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean:watchman": "watchman watch-del-all",
    "clean:node": "sudo rm -rf ./yarn.lock && sudo rm -rf ./node_modules && sudo yarn && sudo chmod -R 777 ./node_modules",
    "clean:ios": "cd ./ios && rm -rf /Pods && rm Podfile.lock && pod install && cd ..",
    "clean:pod": "cd ./ios && pod deintegrate tangopick_app.xcodeproj && pod install",
    "clean:android": "cd ./android && sudo rm -rf ./build && sudo rm -rf ./app/build && ./gradlew clean && cd .. && sudo chmod -R 777 ./android && yarn clean:node"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
