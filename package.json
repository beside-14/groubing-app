{
  "name": "groubing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "clean:watchman": "watchman watch-del-all",
    "clean:node": "sudo rm -rf ./yarn.lock && sudo rm -rf ./node_modules && sudo yarn && sudo chmod -R 777 ./node_modules",
    "clean:ios": "cd ./ios && rm -rf /Pods && rm Podfile.lock && pod install && cd ..",
    "clean:pod": "cd ./ios && pod deintegrate tangopick_app.xcodeproj && pod install",
    "clean:android": "cd ./android && sudo rm -rf ./build && sudo rm -rf ./app/build && ./gradlew clean && cd .. && sudo chmod -R 777 ./android && yarn clean:node"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^7.0.1",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "axios": "^1.3.4",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-actionsheet": "^2.4.2",
    "react-native-calendars": "^1.1295.0",
    "react-native-counters": "^1.2.0",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-modal-datetime-picker": "^15.0.0",
    "react-native-paper": "^5.5.0",
    "react-native-progress-bar": "^0.1.2",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.11",
    "react-router-dom": "^6.10.0",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "17.0.1",
    "@types/react-native": "0.64.1",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "^0.76.7",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
